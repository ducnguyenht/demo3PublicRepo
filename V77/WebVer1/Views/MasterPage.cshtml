    @{
		string seo1 = ""; string desc = ""; string urler = ""; string keyword = "";
		var id = (Request.QueryString["pt"] == null) ? 0 : Convert.ToInt32(Request.QueryString["pt"]);
		SEO seo = new SEOBLL().SEO(id);
		if (seo != null && seo.Name != null)
		{
			if (!String.IsNullOrEmpty(seo.Description))
			{
				desc = Extension.RemoveAllHTMLTags(seo.Description).Length > 150 ? Extension.RemoveAllHTMLTags(seo.Description).Substring(0, 150) + " ..." : Extension.RemoveAllHTMLTags(seo.Description);
			}
			seo1 = seo.Name;
			urler = "http://phukienphanthiet.com" + seo.Url;
			keyword = "";// @Model.Keyword;
		}
		else
		{
			var content = Model.Content as Umbraco.Core.Models.IPublishedContent;
			if (Model.Content.Id.ToString() == "2057")
			{
				seo1 = "PHỤ KIỆN PHAN THIẾT";
				desc = content.GetProperty("moTa") != null ? content.GetProperty("moTa").DataValue.ToString() : "";
				keyword = content.GetProperty("keyword") != null ? content.GetProperty("keyword").DataValue.ToString() : "";
			}
			else
			{
				if (seo1.Length > 32)
				{
					seo1 = Model.Content.Name;
					desc = content.GetProperty("moTa") != null ? content.GetProperty("moTa").DataValue.ToString() : "";
					keyword = content.GetProperty("keyword") != null ? content.GetProperty("keyword").DataValue.ToString() : "";

				}
				else
				{
					seo1 = Model.Content.Name + " - PHỤ KIỆN PHAN THIẾT";
					desc = content.GetProperty("moTa") != null ? content.GetProperty("moTa").DataValue.ToString() : "";
					keyword = content.GetProperty("keyword") != null ? content.GetProperty("keyword").DataValue.ToString() : "";
				}
			}
			desc = Extension.RemoveAllHTMLTags(desc).Length > 150 ? Extension.RemoveAllHTMLTags(desc).Substring(0, 150) + " ..." : Extension.RemoveAllHTMLTags(desc);
		}
    }
